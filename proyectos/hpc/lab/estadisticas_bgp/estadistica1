CONSULTA

SELECT bgpProtocol, time, state, peerIp, peerAs, prefix, asPath, origin, nextHop, localPref, med, community, atomicAgg, aggregator, count(*) AS cantLine FROM log_bgp GROUP BY bgpProtocol, time, state, peerIp, peerAs, prefix, asPath, origin, nextHop, localPref, med, community, atomicAgg, aggregator HAVING cantLine > 1 ;


